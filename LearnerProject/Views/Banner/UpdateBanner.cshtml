@model LearnerProject.Models.Entities.Banner
@{
    ViewBag.Title = "UpdateBanner";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<br />
<h3>Öne Çıkan Güncelleme Sayfası</h3>
<br />

<form method="post">
    @Html.HiddenFor(x=>x.BannerID)
    <label>Başlık</label>
    <br />

    @Html.TextBoxFor(x => x.Title, new { @class = "form-control"})
    <br />
    <button class="btn btn-outline-primary">Güncelle</button>
</form>


